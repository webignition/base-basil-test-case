{
    "name": "webignition/base-basil-test-case",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/webignition/base-basil-test-case",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\BaseBasilTestCase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\BaseBasilTestCase\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR12",
        "static-analysis": "./vendor/bin/phpstan analyse src tests --level=7",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@composer cs",
            "@composer static-analysis",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.2",
        "symfony/panther": "^0.7",
        "phpunit/phpunit": "~8.0",
        "webignition/symfony-panther-dom-crawler-navigator": ">=0.17,<1",
        "webignition/webdriver-element-inspector": ">=0.7,<1",
        "webignition/webdriver-element-mutator": ">=0.3,<1",
        "webignition/basil-models": ">=0.41,<1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpstan/phpstan": "^0.12",
        "webignition/symfony-panther-webserver-runner": ">=0.2,<1"
    }
}
